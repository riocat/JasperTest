	<queryString>
		<![CDATA[SELECT
	*
FROM
	my_sales_order so
LEFT JOIN my_goods_details_info_so gso ON so.objuid = gso.my_sales_order_id
LEFT JOIN my_customer_manufacturer_info mmi ON so.customer = mmi.objuid
LEFT JOIN my_repertory mr ON gso.repertory_code = mr.objuid
WHERE so.order_code = $P{orderCodeSub}]]>
	</queryString>
