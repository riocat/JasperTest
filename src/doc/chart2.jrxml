	<queryString>
		<![CDATA[SELECT
	mr.repertory_name 仓库名称,
	so.goods_name 货品名称,
	SUM(so.num) 货品数量
FROM
	my_repertory mr
LEFT JOIN my_goods_details_info_so so ON mr.objuid = so.repertory_code
GROUP BY
	mr.objuid,
	so.goods_code
ORDER BY mr.repertory_code]]>
